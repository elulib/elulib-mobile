{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "élulib",
  "version": "0.1.0",
  "identifier": "com.elulib.mobile",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "https://app.elulib.com",
    "frontendDist": "https://app.elulib.com"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "élulib",
        "url": "https://app.elulib.com",
        "fullscreen": false,
        "resizable": true,
        "maximized": false,
        "visible": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/ios-1024.png"
    ],
    "category": "Productivity",
    "iOS": {
      "minimumSystemVersion": "14.0"
    },
    "android": {
      "minSdkVersion": 24
    }
  },
  "plugins": {}
}

